<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.AnalizController">
   <children>
      <TableView fx:id="tableView" layoutX="20.0" layoutY="14.0" prefHeight="200.0" prefWidth="139.0">
        <columns>
          <TableColumn prefWidth="75.0" text="X" fx:id="xColumn" />
          <TableColumn fx:id="yColumn" prefWidth="75.0" text="Y" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="button" layoutX="20.0" layoutY="230.0" mnemonicParsing="false" onMouseClicked="#buttonClick" prefHeight="25.0" prefWidth="139.0" text="Verileri Getir" />
      <Button fx:id="btnAnalizEt" layoutX="175.0" layoutY="230.0" mnemonicParsing="false" onMouseClicked="#btnAnalizEtClick" prefHeight="25.0" prefWidth="107.0" text="Analiz Et" visible="false" />
      <TableView fx:id="uzaklıklarTableView" layoutX="175.0" layoutY="14.0" prefHeight="200.0" prefWidth="107.0" visible="false">
        <columns>
          <TableColumn fx:id="uzaklıkColumn" prefWidth="75.0" text="uzaklık" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</AnchorPane>
