<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.AllDataController">
   <children>
      <TableView fx:id="tableView" layoutX="22.0" layoutY="78.0" prefHeight="219.0" prefWidth="460.0">
         <columns>
            <TableColumn fx:id="usernameColumn" prefWidth="75.0" text="User" />
            <TableColumn fx:id="fileNameColumn" prefWidth="75.0" text="File Name" />
            <TableColumn prefWidth="75.0" text="X" fx:id="xColumn" />
            <TableColumn fx:id="yColumn" prefWidth="75.0" text="Y" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="btnCancel" layoutX="115.0" layoutY="353.0" mnemonicParsing="false" onAction="#btnCancelClick" prefHeight="33.0" prefWidth="270.0" style="-fx-background-radius: 10;" text="Back">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Label layoutX="212.0" layoutY="29.0" text="All Data" textFill="#a3a3a3">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Label layoutX="33.0" layoutY="43.0" text="User:" textFill="#a3a3a3">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="lblUsername" layoutX="77.0" layoutY="43.0" prefHeight="21.0" prefWidth="400.0" text="-----" textFill="#a3a3a3">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="btnDownload" layoutX="115.0" layoutY="310.0" mnemonicParsing="false" onAction="#btnDownlaodClick" prefHeight="33.0" prefWidth="270.0" style="-fx-background-radius: 10;" text="Download Images">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
